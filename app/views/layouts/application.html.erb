<!DOCTYPE html>
<html>
<head>
  <title>Webapp</title>
  <%= include_gon %>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<nav class="navbar navbar-default navbar-fixed-top" role="navigation">
	<!-- Brand and toggle get grouped for better mobile display -->
		<div class="navbar-header">
		<a class="navbar-brand" href="http://github.com/SocialCity">SocialCity</a>
		</div>

		<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
			<ul class="nav navbar-nav">
				<li class=""><a href="#">Map View</a></li>
				<li class="dropdown <% concat "active" if @current_action == "map" %>">
					<a href="<%= root_url %>" class="dropdown-toggle" data-toggle="dropdown">Map View<b class="caret"></b></a>
					<ul class="dropdown-menu">
						<li><%= link_to "One Factor", street_map_one_factor_path %></li>
						<li><%= link_to "Two Factors", street_map_two_factor_path %></li>
					</ul>
				</li>
			</ul>
		</div><!-- /.navbar-collapse -->
	</nav>
	<div class="container <%= controller.controller_name %>">
		<div class="row">
			<div class="row analytics-container">
				<div class="">
				</div>
				<div class="row">
					<%= yield %>
				</div>
			</div>
		</div>
	</div>
</body>
</html>
